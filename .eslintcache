iv>\r\n            <div className=\"area--input\">\r\n              <input \r\n                type=\"password\" \r\n                disabled={disabled}\r\n                value={password}\r\n                onChange={(e)=> setPassword(e.target.value)}\r\n                required\r\n              />\r\n            </div>\r\n          </label>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\">Confirmar Senha</div>\r\n            <div className=\"area--input\">\r\n              <input \r\n                type=\"password\" \r\n                disabled={disabled}\r\n                value={confirmPassword}\r\n                onChange={(e)=> setConfirmPassword(e.target.value)}\r\n                required\r\n              />\r\n            </div>\r\n          </label>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\"></div>\r\n            <div className=\"area--input\">\r\n              <button disabled={disabled}>Fazer Cadastro</button>\r\n            </div>\r\n          </label>\r\n        </form>\r\n      </PageArea>\r\n    </PageContainer>\r\n  );\r\n};\r\n\r\nexport default Page;","C:\\Users\\Dionei\\Repository\\n33dful\\frontend\\src\\pages\\SignUp\\styled.js",[],"C:\\Users\\Dionei\\Repository\\n33dful\\frontend\\src\\pages\\Home\\styled.js",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":5,"column":1,"nodeType":"102","endLine":10,"endColumn":3},{"ruleId":"100","severity":1,"message":"101","line":10,"column":1,"nodeType":"102","endLine":30,"endColumn":3},{"ruleId":"100","severity":1,"message":"101","line":83,"column":1,"nodeType":"102","endLine":83,"endColumn":33},{"ruleId":"103","severity":1,"message":"104","line":29,"column":6,"nodeType":"105","endLine":29,"endColumn":8,"suggestions":"106"},"no-native-reassign",["107"],"no-negated-in-lhs",["108"],"import/no-anonymous-default-export","Assign arrow function to a variable b