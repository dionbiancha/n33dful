 Page = () => {\r\n  const api = useApi();\r\n  const { id } = useParams();\r\n\r\n  const [loading, setLoading] = useState(true);\r\n  const [adInfo, setAdInfo] = useState({});\r\n\r\n  const formatDate = (date) => {\r\n    let cDate = new Date(date);\r\n\r\n    let months = ['janeiro', 'fevereiro', 'março', 'abril', 'maio', 'junho', 'julho', 'agosto', 'setembro', 'outubro', 'novembro', 'dezembro'];\r\n    let cDay = cDate.getDate();\r\n    let cMoths = cDate.getMonth();\r\n    let cYear = cDate.getFullYear();\r\n\r\n    return `${cDay} de ${months[cMoths]} de ${cYear}`;\r\n  }\r\n\r\n  useEffect(() => {\r\n    const getAdInfo = async (id) => {\r\n      const json = await api.getAd(id, true);\r\n      setAdInfo(json);\r\n      setLoading(false);\r\n    }\r\n    getAdInfo(id);\r\n  }, []);\r\n\r\n  return(\r\n    <PageContainer>\r\n      {adInfo.category &&\r\n        <BreadChumb>\r\n          Você está aqui:\r\n          <Link to=\"/\">Home</Link> \r\n          /\r\n          <Link to={`/ads?state=${adInfo.stateName}`}>{adInfo.stateName}</Link>\r\n          /\r\n          <Link to={`/ads?state=${adInfo.stateName}&cat=${adInfo.category.name}`}>{adInfo.category.slug}</Link>\r\n          / {adInfo.title}\r\n        </BreadChumb>\r\n      }\r\n      <PageArea>\r\n        <div className=\"leftSide\">\r\n          <div className=\"box\">\r\n            <div className=\"adImage\">\r\n              {loading && <Fake height={300}/>}\r\n              {adInfo.images &&\r\n                <Slide>\r\n                  {adInfo.images.map((img, k) => \r\n                    <div className=\"each-slide\" key={k}>\r\n                      <img src={img} alt=\"\" />\r\n                    </div>\r\n                  )}\r\n                </Slide>\r\n              }\r\n            </div>\r\n            <div className=\"adInfor\">\r\n              <div className=\"adName\">\r\n                {loading && <Fake height={20}/>}\r\n                {adInfo.title &&\r\n                  <h2>{adInfo.title}</h2>\r\n                }\r\n                {adInfo.dateCreated && \r\n                <small>Criado em {formatDate(adInfo.dateCreated)}</small>\r\n                }\r\n              </div>\r\n              <div className=\"adDescription\">\r\n                {loading && <Fake height={100}/>}\r\n                {adInfo.description}\r\n                <hr/>\r\n                {adInfo.views && \r\n                  <small>Visualizações: {adInfo.views}</small>\r\n                }\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div className=\"rightSide\">\r\n          <div className=\"box box--padding\">\r\n            {loading && <Fake height={20}/>}\r\n            {adInfo.priceNegotiable &&\r\n              \"Preço Negociável\"\r\n            }\r\n            {!adInfo.priceNegotiable && adInfo.price &&\r\n              <div className=\"price\">\r\n                Preço: \r\n                <span>\r\n                  R${adInfo.price}\r\n                </span>\r\n              </div>\r\n            }\r\n          </div>\r\n          {loading && <Fake height={50}/>}\r\n          {adInfo.userInfo &&\r\n            <>\r\n              <a href={`mailto:${adInfo.userInfo.email}`} target=\"_blank\" className=\"contactSellerLink\">Fale com o vendedor</a>\r\n              <div className=\"createdBy box box--padding\">\r\n                <strong>{adInfo.userInfo.name}</strong>\r\n                <small>E-mail: {adInfo.userInfo.email}</small>\r\n                <small>Estado: {adInfo.stateName}</small>\r\n              </div>\r\n            </>\r\n          }\r\n        </div>\r\n      </PageArea>\r\n      <OthersArea>\r\n        {adInfo.others && \r\n          <>\r\n            <h2>Outras ofertas do vendedor</h2>\r\n            <div className=\"list\">\r\n              {adInfo.others.map((i, k) => \r\n                <AdItem key={k} data={i}/>\r\n                )\r\n              }\r\n            </div>\r\n          </>\r\n        }\r\n      </OthersArea>\r\n    </PageContainer>\r\n  );\r\n};\r\n\r\nexport default Page;","C:\\Users\\Dionei\\Repository\\n33dful\\frontend\\src\\pages\\AdPage\\styled.js",[],"C:\\Users\\Dionei\\Repository\\n33dful\\frontend\\src\\components\\RouteHandler.js",["142"],"import React from 'react';\r\nimport { Route, Redirect } from 'react-router-dom'\r\nimport {isLogged} from '../helpers/AuthHandler';\r\n\r\nexport default ({children, ...rest}) => {\r\n  let logged = isLogged();\r\n  let authorized = (rest.privated && !logged) ? false : true;\r\n\r\n  return (\r\n    <Route \r\n      {...rest}\r\n      render={()=>\r\n        authorized ? children : <Redirect to=\"/signin\" />\r\n      }\r\n    />\r\n  );\r\n}","C:\\Users\\Dionei\\Repository\\n33dful\\frontend\\src\\pages\\AddAd\\index.js",["143"],"import React, { useState, useRef, useEffect } from 'react';\r\nimport { useHistory } from 'react-router-dom';\r\nimport MaskedInput from 'react-text-mask';\r\nimport createNumberMask from 'text-mask-addons/dist/createNumberMask';\r\nimport { PageArea } from './styled';\r\nimport { PageContainer, PageTitle, ErrorMessage } from '../../components/MainComponents';\r\nimport useApi from '../../helpers/N33dfulAPI';\r\n\r\n\r\nconst Page = () => {\r\n  const api = useApi();\r\n  const fileField = useRef();\r\n  const history = useHistory();\r\n\r\n  const [categories, setCategories] = useState([]);\r\n\r\n  const [title, setTitle] = useState('');\r\n  const [category, setCategory] = useState('');\r\n  const [price, setPrice] = useState('');\r\n  const [priceNegotiable, setPriceNegotiable] = useState(false);\r\n  const [desc, setDesc] = useState('');\r\n\r\n  const [disabled, setDisabled] = useState(false);\r\n  const [error, setError] = useState('');\r\n\r\n  useEffect(()=>{\r\n    const getCategories = async () => {\r\n      const cats = await api.getCategories();\r\n      setCategories(cats);\r\n    }\r\n    getCategories();\r\n  }, []);\r\n\r\n\r\n  const handleSubmit = async(e) => {\r\n    e.preventDefault();\r\n    setDisabled(true);\r\n    setError('');\r\n\r\n    let errors = [];\r\n\r\n    if(!title.trim()) {\r\n      errors.push('Sem titulo');\r\n    }\r\n\r\n    if(!category) {\r\n      errors.push('Sem categoria');\r\n    }\r\n\r\n    if(errors.length === 0) {\r\n      const fData = new FormData();\r\n      fData.append('title', title);\r\n      fData.append('price', price);\r\n      fData.append('priceneg', priceNegotiable);\r\n      fData.append('desc', desc);\r\n      fData.append('cat', category);\r\n\r\n      if(fileField.current.files.length > 0) {\r\n        for(let i = 0; i < fileField.current.files.length; i++) {\r\n          fData.append('img', fileField.current.files[i]);\r\n        }\r\n      }\r\n\r\n      const json = await api.addAd(fData);\r\n\r\n      if(!json.error) {\r\n        history.push(`/ad/${json.id}`);\r\n        return;\r\n      } else {\r\n        setError(json.error);\r\n      }\r\n\r\n    } else {\r\n      setError(errors.join(\"\\n\"))\r\n    }\r\n\r\n    setDisabled(false);\r\n  };\r\n\r\n  const priceMask = createNumberMask({\r\n    prefix:'R$ ',\r\n    includeThousandsSeparator: true,\r\n    thousandsSeparatorSymbol: '.',\r\n    allowDecimal: true,\r\n    decimalSymbol: ','\r\n  });\r\n\r\n  return(\r\n    <PageContainer>\r\n      <PageTitle>Postar um anúncio</PageTitle>\r\n      <PageArea>\r\n        {error &&\r\n          <ErrorMessage>\r\n            {error}\r\n          </ErrorMessage>\r\n        }\r\n        <form onSubmit={handleSubmit}>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\">Titulo</div>\r\n            <div className=\"area--input\">\r\n              <input \r\n                type=\"text\" \r\n                disabled={disabled}\r\n                value={title}\r\n                onChange={(e)=> setTitle(e.target.value)}\r\n                required\r\n              />\r\n            </div>\r\n          </label>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\">Categoria</div>\r\n            <div className=\"area--input\">\r\n              <select\r\n                disabled={disabled}\r\n                onChange={(e)=>setCategory(e.target.value)}\r\n                required\r\n              >\r\n                <option></option>\r\n                {categories && categories.map(i => \r\n                  <option key={i._id} value={i._id}>\r\n                    {i.name}\r\n                  </option>\r\n                )}\r\n              </select>\r\n            </div>\r\n          </label>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\">Preço</div>\r\n            <div className=\"area--input\">\r\n              <MaskedInput \r\n                mask={priceMask}\r\n                placeholder=\"R$\"\r\n                disabled={disabled || priceNegotiable}\r\n                value={price}\r\n                onChange={e => setPrice(e.target.value)}\r\n              />\r\n            </div>\r\n          </label>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\">Preço negociavel</div>\r\n            <div className=\"area--input\">\r\n              <input \r\n                type=\"checkbox\"\r\n                disabled={disabled}\r\n                checked={priceNegotiable}\r\n                onChange={e => setPriceNegotiable(!priceNegotiable)}\r\n              />\r\n            </div>\r\n          </label>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\">Descrição</div>\r\n            <div className=\"area--input\">\r\n              <textarea\r\n                disabled={disabled}\r\n                value={desc}\r\n                onChange={e => setDesc(e.target.value)}\r\n              ></textarea>\r\n            </div>\r\n          </label>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\">Imagens (1 ou mais)</div>\r\n            <div className=\"area--input\">\r\n              <input \r\n                type=\"file\"\r\n                disabled={disabled}\r\n                ref={fileField}\r\n                multiple\r\n              />\r\n            </div>\r\n          </label>\r\n          <label className=\"area\">\r\n            <div className=\"area--title\"></div>\r\n            <div className=\"area--input\">\r\n              <button disabled={disabled}>Adicionar Anúncio</button>\r\n            </div>\r\n          </label>\r\n        </form>\r\n      </PageArea>\r\n    </PageContainer>\r\n  );\r\n};\r\n\r\nexport default Page;","C:\\Users\\Dionei\\Repository\\n33dful\\frontend\\src\\pages\\AddAd\\styled.js",[],"C:\\Users\\Dionei\\Repository\\n33dful\\frontend\\src\\pages\\Ads\\index.js",["144","145","146","147"],"C:\\Users\\Dionei\\Repository\\n33dful\\frontend\\src\\pages\\Ads\\styled.js",[],{"ruleId":"148","replacedBy":"149"},{"ruleId":"150","replacedBy":"151"},{"ruleId":"152","severity":1,"message":"153","line":5,"column":1,"nodeType":"154","endLine":10,"endColumn":3},{"ruleId":"152","severity":1,"message":"153","line":15,"column":1,"nodeType":"154","endLine":44,"endColumn":3},{"ruleId":"155","severity":1,"message":"156","line":42,"column":6,"nodeType":"157","endLine":42,"endColumn":8,"suggestions":"158"},{"ruleId":"152","severity":1,"message":"153","line":130,"column":1,"nodeType":"154","endLine":130,"endColumn":33},{"ruleId":"155","severity":1,"message":"156","line":29,"column":6,"nodeType":"157","endLine":29,"endColumn":8,"suggestions":"159"},{"ruleId":"152","severity":1,"message":"153","line":5,"column":1,"nodeType":"154","endLine":29,"endColumn":2},{"ruleId":"155","severity":1,"message":"160","line":36,"column":6,"nodeType":"157","endLine":36,"endColumn":8,"suggestions":"161"},{"ruleId":"162","severity":1,"message":"163","line":105,"column":15,"nodeType":"164","endLine":105,"endColumn":105},{"ruleId":"152","severity":1,"message":"153","line":5,"co